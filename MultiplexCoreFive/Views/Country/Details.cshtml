@model MultiplexCoreFive.Models.Country

<div class="container">
    <div class="row">
        <div class="col-lg-12">
            <div class="card">
                <div class="card-header">
                    Country Details
                </div>
                <div class="card-body">
                    @{
                        if (Model != null)
                        {
                            <div class="row">
                                <div class="form-inline">
                                    <label>Name: </label>
                                    <h5>@Model.Name</h5>
                                </div>
                            </div>
                            <div class="row">
                                <div class="form-inline">
                                    <label>Short Name: </label>
                                    <h5>@Model.ShortName</h5>
                                </div>
                            </div>
                        }
                    }
                </div>
            </div>
        </div>
    </div>
</div>
